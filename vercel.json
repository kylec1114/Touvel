{
  "$schema": "https://openapi.vercel.sh",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
